<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffcccccc" />
            <padding android:left="7.0dip" android:top="7.0dip" android:right="7.0dip" android:bottom="7.0dip" />
            <stroke android:width="1.0px" android:color="#ff999999" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffcccccc" />
            <padding android:left="7.0dip" android:top="7.0dip" android:right="7.0dip" android:bottom="7.0dip" />
            <stroke android:width="1.0px" android:color="#ff999999" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffffffff" />
            <padding android:top="7.0dip" android:right="7.0dip" android:bottom="7.0dip" />
            <stroke android:width="1.0px" android:color="#ffcccccc" />
        </shape>
    </item>
</selector>